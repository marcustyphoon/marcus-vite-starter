{
  "name": "marcus-vite-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "npm": "please-use-pnpm",
    "yarn": "please-use-pnpm"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "eslintCheck": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "typeCheck": "tsc --noEmit",
    "prettierCheck": "prettier --check . \"!**/*.{md,json,yaml,yml}\"",
    "test": "concurrently -g -c auto pnpm:eslintCheck pnpm:typeCheck pnpm:prettierCheck"
  },
  "dependencies": {
    "@vanilla-extract/css": "^1.14.2",
    "@vanilla-extract/vite-plugin": "^4.0.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.13"
  },
  "devDependencies": {
    "@types/react": "^18.2.78",
    "@types/react-dom": "^18.2.25",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5",
    "vite": "^5.2.0"
  }
}
